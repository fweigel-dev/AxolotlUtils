{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:display_context",
    "cases": [
      {
        "when": "gui",
        "model": {
          "type": "minecraft:model",
          "model": "minecraft:item/axolotl_bucket"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:select",
      "property": "minecraft:component",
      "component": "minecraft:axolotl/variant",
      "fallback": {
        "type": "minecraft:model",
        "model": "minecraft:item/axolotl_bucket"
      },
      "cases": [
        {
          "when": "wild",
          "model": {
            "type": "minecraft:model",
            "model": "axolotlutils:item/axolotl_bucket_wild"
          }
        },
        {
          "when": "gold",
          "model": {
            "type": "minecraft:model",
            "model": "axolotlutils:item/axolotl_bucket_gold"
          }
        },
        {
          "when": "cyan",
          "model": {
            "type": "minecraft:model",
            "model": "axolotlutils:item/axolotl_bucket_cyan"
          }
        },
        {
          "when": "blue",
          "model": {
            "type": "minecraft:model",
            "model": "axolotlutils:item/axolotl_bucket_blue"
          }
        }
      ]
    }
  }
}
